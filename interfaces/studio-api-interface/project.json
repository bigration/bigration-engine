{
  "name": "interfaces-studio-api-interface",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "interfaces/studio-api-interface/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npm install"
          }
        ],
        "cwd": "interfaces/studio-api-interface"
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
